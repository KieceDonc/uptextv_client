  {
  "manifest_version": 2,
  "name": "uptextv",
  "version": "2.5.9",

  "description": "Pin and categorise your favourite channels on twitch",

  "permissions": ["*://*.twitch.tv/*"],
  "web_accessible_resources": ["uptextv.js"],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["*://*.twitch.tv/*"],
      "js": ["./run.js"]
    }
  ]
}
